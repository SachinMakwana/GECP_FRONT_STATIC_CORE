@model GECP_Front_End_Static.Models.HomePageModelVM
@{
    ViewData["Title"] = "Home Page";
    int newscount = Model.newsModelVM.Count();
}

@Html.Partial("_slider",Model.masterSliderVM)

<div class="clearfix margin_top5"></div>
<div class="featured_section13">
    <div class="full-container">
        
            <h1 class=" animate" data-anim-type="fadeIn" data-anim-delay="100">
                Welcome to Government Engineering College,Patan
                <div class="clearfix margin_top5"></div>
                @* <div class="divider_line11belownews"></div> *@
            </h1>

        <div class="New_container">

            <div class="updates_section">
           

                <ul>
                    <li class="tab-heading">UPDATES</li>
                </ul><!-- /# end tab links -->

                <div style="min-height:270px;">
                    <div>
                        <marquee scrollamount="3" direction="up" onMouseOver="this.stop()" onMouseOut="this.start()" style="min-height:225px;">
                            <ul class="newssection">
                                @if (Model.marqueeModelVM == null || !Model.marqueeModelVM.Any())
                                {
                                    <li><strong>NO UPDATES FOUND</strong></li>
                                }
                                else
                                {
                                    foreach (var item in Model.marqueeModelVM)
                                    {
                                        if (item.IsFile)
                                        {
                                            <li><a href="@Url.Content(item.FileLink)" target="_blank">@item.Title</a> </li>
                                        }
                                        else if (string.IsNullOrWhiteSpace(item.ActionName) && string.IsNullOrWhiteSpace(item.ControllerName))
                                        {
                                            <li><a href="@Url.Action("NEWSDetail", "Institute", new { @id = item.ID })">@item.Title</a></li>
                                        }
                                        else
                                        {
                                            <li><a href="@Url.Action(item.ActionName, item.ControllerName)"> @item.Title </a></li>
                                        }
                                    }
                                }
                            </ul>
                        </marquee>
                    </div>
                </div>
            </div>
            

            <div class="one_halfs">
                <div class="one_fourth">

                    <img src="@Url.Content("~/images/Home/icon1.jpg")" alt="" class="cirimg animate" data-anim-type="zoomIn" data-anim-delay="100" />

                    <h4 class="color2">Five UG Engineering Programs</h4>

                    <br /><br />
                    @*<a href="#" class="but_calendar">Read More</a>*@

                </div>
                <!-- end section -->

                <div class="one_fourth">

                    <img src="@Url.Content("~/images/Home/icon2.jpg")" alt="" class="cirimg animate" data-anim-type="zoomIn" data-anim-delay="200" />

                    <h4 class="color2">Well Qualified Faculties</h4>

                    <br /><br />
                    @*<a href="#" class="but_calendar">Read More</a>*@

                </div>
                <!-- end section -->

                <div class="one_fourth">

                    <img src="@Url.Content("~/images/Home/icon3.jpg")" alt="" class="cirimg animate" data-anim-type="zoomIn" data-anim-delay="300" />

                    <h4 class="color2">Center of Excellence</h4>

                    <br /><br />
                    @*<a href="#" class="but_calendar">Read More</a>*@

                </div>
                <!-- end section -->

                <div class="one_fourth last">

                    <img src="@Url.Content("~/images/Home/icon4.jpg")" alt="" class="cirimg animate" data-anim-type="zoomIn" data-anim-delay="400" />

                    <h4 class="color2">Holistic Development of Students</h4>

                    <br /><br />
                    @*<a href="#" class="but_calendar">Read More</a>*@

                </div>
            <!-- end section -->
            </div>

            <div class="updates_section">


                <ul>
                    <li class="tab-heading">ACTIVITIES</li>
                </ul>

                <div style="min-height:270px;">
                    <div>
                        <marquee scrollamount="3" direction="up" onMouseOver="this.stop()" onMouseOut="this.start()" style="min-height:225px;">
                            <ul class="newssection">
                                @if (Model.activitymarqueeModelVM == null || !Model.activitymarqueeModelVM.Any())
                                {
                                    <li><strong>NO ACTIVITIES FOUND</strong></li>
                                }
                                else
                                {
                                    foreach (var item in Model.activitymarqueeModelVM)
                                    {
                                        if (item.IsFile)
                                        {
                                            <li><a href="@Url.Content(item.FileLink)" target="_blank">@item.Title</a> </li>
                                        }
                                        else if (string.IsNullOrWhiteSpace(item.ActionName) && string.IsNullOrWhiteSpace(item.ControllerName))
                                        {
                                            <li><a href="@Url.Action("NEWSDetail", "Institute", new { @id = item.ID })">@item.Title</a></li>
                                        }
                                        else
                                        {
                                            <li><a href="@Url.Action(item.ActionName, item.ControllerName)"> @item.Title </a></li>
                                        }
                                    }
                                }
                            </ul>
                        </marquee>
                    </div>
                </div>


            </div><!-- end section -->
        </div>
    </div>
</div>
<!-- end featured section 13 -->
<div class="clearfix margin_top5"></div>

<div class="vision_section">
        <div class="clearfix margin-top5"></div>

        <div class="one_half stcode_title4">

        <h3 class="text-center"><span class="text"><i class="fa fa fa-institution" aria-hidden="true"></i><strong>  VISION</strong></span></h3>


          @*   <div class="vision-container">
