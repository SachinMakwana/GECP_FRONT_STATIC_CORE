@section tabs {

    <script src="@Url.Content("~/js/tabs2/index.js")"></script>
    <script>
        $('.accordion, .tabs').TabsAccordion({
            hashWatch: true,
            pauseMedia: true,
            responsiveSwitch: 'tablist',
            saveState: sessionStorage,
        });
    </script>
    <script defer src="@Url.Content("~/js/flexslider/jquery.flexslider.js")"></script>
    <script defer src="@Url.Content("~/js/flexslider/custom.js")"></script>
    @* <script type="text/javascript" src="@Url.Content("~/js/tabs/tabwidget/tabwidget.js")"></script> *@
    <script src="@Url.Content("~/js/tabs/assets/js/responsive-tabs.min.js")" type="text/javascript"></script>
}

@section master_slider {
    <script src="@Url.Content("~/js/modalwindow/jquery.simplePopup.js")" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function () {

            $('.show1');
            $('#pop1').simplePopup();
           
        });
    </script>
    <script src="@Url.Content("~/js/masterslider/masterslider.min.js")"></script>
    <script type="text/javascript">
        (function ($) {
            "use strict";
            var slider = new MasterSlider();
            // adds Arrows navigation control to the slider.
            slider.control('arrows');
            slider.control('bullets');

            slider.setup('masterslider', {
                width: 1400, // slider standard width
                height: 680, // slider standard height
                space: 0,
                speed: 45,
                layout: 'fullwidth',
                loop: true,
                preload: 0,
                autoplay: true,
                view: "parallaxMask"
            });
        })(jQuery);
    </script>
    <script type="text/javascript">
        (function ($) {
            "use strict";
            var slider = new MasterSlider();

            slider.setup('masterslider2', {
                width: 1400, // slider standard width
                height: 580, // slider standard height
                space: 0,
                speed: 27,
                layout: 'fullwidth',
                loop: true,
                preload: 0,
                autoplay: true,
                view: "basic",
            });
        })(jQuery);
    </script>
}

