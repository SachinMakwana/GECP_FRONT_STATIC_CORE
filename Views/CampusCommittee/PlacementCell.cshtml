@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Placement Cell";
}

<div class="clearfix"></div>


<div class="page_title3">
    <div class="container">

        <h1>
            <br> Placement Cell<em>Government Engineering College</em>
            <div class="line"></div>
            <br>
        </h1>

    </div>
</div>
<div class="clearfix"></div>


<div class="content_fullwidth">
    <div class="featured_section55 animate" data-anim-type="fadeIn" data-anim-delay="100">
        <div class="container">

            <h2 class="caps">About GECP Placement Cell</h2>

            <br /><br />

            <div class="one_third">

                <h4 class="color3 margin_bottom2">GECP Placement Policy</h4>
                <p class="bigtfont">The Training and Placement Cell (T&P Cell or TPC) at Government Engineering College - Patan, works professionally with the Industry to place its students in each and every sector.</p>

            </div>
            <!-- end section -->

            <div class="one_third">

                <img src="@Url.Content("~/images/site-img59.jpg")" alt="" class="rimg" />

            </div>
            <!-- end section -->

            <div class="one_third last">

                <h4 class="color3 margin_bottom2">GECP Measures</h4>
                <p class="bigtfont">The T&P Cell handle all aspects of the campus recruitment at institute and also helps in enhancing the skills of the students as per the demands of the industry by conducting many numbers of seminars & workshops including KCG’s Finishing School (GoG) Programs.</p>

            </div>

            <!-- end section -->

        </div>
    </div>
</div>
<!-- end featured section 55 -->

<div class="margin_top2"></div>

<div class="featured_section14">
    <div class="container">

        <div class="one_half animate" data-anim-type="fadeInLeft" data-anim-delay="300">

            <img src="@Url.Content("~/images/icon-img13.png")" alt="" />
            <br /><br />
            <h3 class="white">Vision</h3>

            <p>Enhance the employability skills among the students to meet out the corporate expectations and to furnish them with professional skills and guiding them towards the bright future and career.</p>
            <br /><br />


        </div>
        <!-- end section -->

        <div class="one_half last animate" data-anim-type="fadeInRight" data-anim-delay="300">

            <img src="@Url.Content("~/images/icon-img12.png")" alt="" />
            <br /><br />
            <h3 class="white">Mission</h3>

            <p>To train students for the needs of industries and to partner with renowned organizations from industry & organize recruitment drives for our students within and outside the campus.</p>
            <br /><br />


        </div>
        <!-- end section -->

    </div>
</div>
<!-- end featured section 14 -->







<div class="parallax_section12">
    <div class="container">

        <div class="counters7 animate" data-anim-type="fadeIn" data-anim-delay="300">

            <div class="one_fifth">
                <i class="fa fa-calendar"></i> <span id="target21">0</span>
                <h4>Annual Placement</h4>
            </div>
            <!-- end section -->

            <div class="one_fifth">
                <i class="fa fa-money"></i> <span id="target22">0</span>
                <h4>Dream Packages</h4>
            </div>
            <!-- end section -->

            <div class="one_fifth">
                <i class="fa fa-trophy"></i> <span id="target23">0</span>
                <h4>Total Recruiters</h4>
            </div>
            <!-- end section -->

            <div class="one_fifth">
                <i class="fa fa-paper-plane"></i> <span id="target24">0</span>
                <h4>International Company</h4>
            </div>
            <!-- end section -->

            <div class="one_fifth last">
                <i class="fa fa-thumbs-up"></i> <span id="target5">0</span>
                <h4>Confirm Placement</h4>
            </div>
            <!-- end section -->


        </div>


    </div>
</div>
<!-- end parallax section 12 -->


