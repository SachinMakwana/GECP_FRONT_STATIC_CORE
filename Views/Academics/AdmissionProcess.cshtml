@model IntakeVM
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Admission Process";
}


<div class="clearfix"></div>


<div class="page_title3 engg_title">
    <div class="container">

        <h1>
            Admission Process <em>Government Engineering College</em>
            <div class="line"></div>
        </h1>

    </div>
</div>
<div class="featured_section63">
    <div class="container">



        <h4 class="roboto">
            Government Engineering College, Patan (GECP), is a government aided educational institution located in Katpur, near Patan, Gujarat. The institute offers undergraduate level engineering courses. The college was founded in April 2004 and is
            affiliated to Gujrat Technological University (GTU).
        </h4>

    </div>
</div>

<!-- end featured section 63 -->
<div class="parallax_section12">
    <div class="container">
        <div class="featured_section13">

            <h1 class="title22 animate" data-anim-type="fadeIn" data-anim-delay="100" style="color: white;">
                Students Intake

            </h1>
        </div>

        <div class="counters7 animate" data-anim-type="fadeIn" data-anim-delay="300">

            <div class="one_fifth">
                <i class="fa fa-microchip"></i> <span class="target120" id="target120">@Model.EC</span>
                <h4>EC</h4>
            </div>
            <!-- end section -->

            <div class="one_fifth">
                <i class="fa fa-desktop"></i> <span class="target121" id="target121">@Model.Computer</span>
                <h4>Computer</h4>
            </div>
            <!-- end section -->

            <div class="one_fifth">
                <i class="fa fa-bolt"></i> <span class="target122" id="target122">@Model.Electrical</span>
                <h4>Electrical</h4>
            </div>
            <!-- end section -->

            <div class="one_fifth">
                <i class="fas fa-hard-hat"></i> <span class="target123" id="target123">@Model.Civil</span>
                <h4>Civil</h4>
            </div>
            <!-- end section -->

            <div class="one_fifth last">
                <i class="fa fa-wrench"></i> <span class="target124" id="target124">@Model.Mechanical</span>
                <h4>Mechanical</h4>
            </div>
            <!-- end section -->


        </div>


    </div>
</div>
<div class="margin_top2"></div>

<div class="featured_section37">
    <div class="container">

        <!-- end section -->

        <div class="two_third">

            <br />

            <h2>Admission Process</h2>

            <div class="ibox animate" data-anim-type="fadeInUp" data-anim-delay="100">

                <i class="fa fa-check"></i>
                <h5>Only online mode of application is allowed. Candidates can register and apply through GUJCET website.</h5>

            </div>
            <!-- end section -->

            <div class="ibox animate" data-anim-type="fadeInUp" data-anim-delay="200">

                <i class="fa fa-check"></i>
                <h5>Registration fees of INR 350 can be paid at the “Help Centre” while purchasing the information booklet and PIN.</h5>


            </div>
            <!-- end section -->

            <div class="ibox animate" data-anim-type="fadeInUp" data-anim-delay="300">

                <i class="fa fa-check"></i>
                <h5>10+2 in science stream with minimum 45% marks for B. Tech programs.</h5>


            </div>
            <!-- end section -->

            <div class="ibox animate" data-anim-type="fadeInUp" data-anim-delay="400">

                <i class="fa fa-check"></i>
                <h5>Only GUJCET or JEE Main qualified candidates are eligible for B. Tech counselling and admission.</h5>


            </div>
            <!-- end section -->
            <div class="ibox animate" data-anim-type="fadeInUp" data-anim-delay="400">

                <i class="fa fa-check"></i>
                <h5>All candidates are selected for admission through GUJCET counselling only.</h5>


            </div>
            <!-- end section -->
        </div>
        <div class="one_third last">

            <div class="ms-phone-template">
                <div class="ms-phone-cont">
                    <img src="@Url.Content("~/images/hiearchy.PNG")" class="ms-phone-bg" height="620px" />
                </div>
            </div>
        </div>


    </div>
</div>

