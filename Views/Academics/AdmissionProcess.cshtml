@section scripts{
    <script type="text/javascript" src="@Url.Content("~/js/masterslider/jquery.easing.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/js/accordion/jquery.accordion.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/js/accordion/custom.js")"></script>
    <script>
        (function ($) {
            "use strict";

            $(document).ready(function () {
                var target120 = $("#target120").text();
                var target121 = $("#target121").text();
                var target122 = $("#target122").text();
                var target123 = $("#target123").text();
                var target124 = $("#target124").text();

                $('.target120').animateNumber({
                    number: target120,

                    numberStep: function (now, tween) {
                        var floored_number = Math.floor(now),
                            target = $(tween.elem);

                        target.text(floored_number);
                    }
                },
                    12000
                )

                $('.target121').animateNumber({
                    number: target121,

                    numberStep: function (now, tween) {
                        var floored_number = Math.floor(now),
                            target = $(tween.elem);

                        target.text(floored_number);
                    }
                },
                    12000
                )
                $('.target122').animateNumber({
                    number: target122,

                    numberStep: function (now, tween) {
                        var floored_number = Math.floor(now),
                            target = $(tween.elem);

                        target.text(floored_number);
                    }
                },
                    12000
                )
                $('.target123').animateNumber({
                    number: target123,

                    numberStep: function (now, tween) {
                        var floored_number = Math.floor(now),
                            target = $(tween.elem);

                        target.text(floored_number);
                    }
                },
                    12000
                )

                $('.target124').animateNumber({
                    number: target124,

                    numberStep: function (now, tween) {
                        var floored_number = Math.floor(now),
                            target = $(tween.elem);

                        target.text(floored_number);
                    }
                },
                    12000
                )
            });

        })(jQuery);
    </script>

    <!-- end section -->
}

